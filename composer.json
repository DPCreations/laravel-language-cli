{
    "name": "dpcreations/laravel-language-cli",
    "description": "A handy little CLI for managing and simplifying your work with Laravel translations.",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Poulsen",
            "email": "daniel@dpcreations.dk",
            "homepage": "https://dpcreations.dk"
        }
    ],
    "homepage": "https://github.com/dpcreations/laravel-language-cli",
    "keywords": [
        "Laravel",
        "Laravel language",
        "Laravel language CLI"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "DPCreations\\LaravelLanguageCli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DPCreations\\LaravelLanguageCli\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DPCreations\\LaravelLanguageCli\\laravelLanguageCliServiceProvider"
            ]
        }
    }
}
